{
  "name": "FlareApp To Trello",
  "description": "FlareApp To Trello",
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    "16": "/icon/icon.png",
    "48": "/icon/icon.png",
    "128": "/icon/icon.png"
  },
  "browser_action": {
    "default_icon": "/icon/icon.png",
    "default_title": "Zavgar Online",
    "default_popup": "settings.html"
  },
  "content_scripts": [
    {
      "matches": ["https://flareapp.io/*", "https://trello.com/*"],
      "js": ["injection.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "notifications",
    "background"
  ],
  "author": "Алексей Власов"
}
